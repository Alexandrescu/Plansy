@if (Model != null)
{
    <div class="landing-page-content" style="margin-top: 80px;">
        <div class="page-content">
            <div class="row" style="margin-bottom: 20px;">
                <div class="col-md-6">
                    <div class="row box6">
                        <div class="col-sm-6">
                            <div class="count-box">
                                68
                            </div>
                            <p class="count-box-text">
                                @Resource.TotalProviders
                            </p>
                        </div>

                        <div class="col-sm-6">
                            <div class="count-box">
                                532
                            </div>
                            <p class="count-box-text">
                                @Resource.TotalClients
                            </p>
                        </div>

                        <div class="col-sm-6">
                            <div class="count-box">
                                21
                            </div>
                            <p class="count-box-text">
                                @Resource.AppointmentsToday
                            </p>
                        </div>

                        <div class="col-sm-6">
                            <div class="count-box">
                                48
                            </div>
                            <p class="count-box-text">
                                @Resource.AppointmentsThisWeek
                            </p>
                        </div>

                        <div class="col-md-12" style="margin-top: 10px">
                            <div class="well well-sm" style="background-color: #d8dadb; color:#475869;">
                                @Resource.LandingPage_RequestOfferInfo: @DateTime.Now.ToString("dd-MM-yyyy")
                            </div>
                        </div>
                    </div>

                </div>
                <div class="col-md-6">

                    <h3>
                        @Resource.LandingPage_WhyTitle
                    </h3>
                    <ul class="list-group">
                        <li class="list-group-item ">@Resource.LandingPage_Why1</li>
                        <li class="list-group-item ">@Resource.LandingPage_Why2</li>
                        <li class="list-group-item ">@Resource.LandingPage_Why3</li>
                        <li class="list-group-item ">@Resource.LandingPage_Why4</li>
                        <li class="list-group-item ">@Resource.LandingPage_Why5</li>
                        <li class="list-group-item ">@Resource.LandingPage_Why6</li>
                    </ul>
                </div>
            </div>



            <div class="row" style="margin-bottom: 20px;">
                <div class="col-md-12">
                    <hr style="margin-bottom: 50px;" />
                     <div class="row" style="margin-bottom: 20px;">
                         <div class="col-md-6">
                             <h3 style="margin-bottom: 20px;">@Resource.LandingPage_Mobile1?</h3>
                             <h4><b>@Resource.LandingPage_Mobile2 </b>@Resource.LandingPage_Mobile3!</h4>
                         </div>
                        <div class="col-md-6">
                            <p>
                                <a href="https://itunes.apple.com/us/app/ma-programez/id1016292213" target="_blank" title="Available on the AppStore">
                                    <img src="~/Content/Images/appstore.png" alt="AppStore" style="width: 40%; height: auto; margin:20px;" />
                                </a>
                                <a href="https://play.google.com/store/apps/details?id=com.spinit.maprogramez" target="_blank" title="Android App on Google play">
                                    <img src="~/Content/Images/googleplaystore.png" alt="AppStore" style="width: 40%; height: auto; margin:20px;" />
                                </a>
                            </p>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
}
